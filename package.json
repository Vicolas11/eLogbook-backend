{
  "name": "elogbook-backend",
  "version": "1.0.0",
  "description": "elogbook Core Backend",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "dev": "cross-env NODE_ENV=development nodemon",
    "copy-schemas": "cpx src/**/*.graphql build",
    "vercel": "cpx vercel.json build",
    "build": "tsc && npm run copy-schemas && npm run vercel",
    "start": "node ./build/index.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "reset": "prisma migrate reset",
    "status": "prisma migrate status",
    "deploy": "prisma migrate deploy",
    "docs": "npx prisma-docs-generator serve",
    "format": "npx prisma format",
    "studio": "npx prisma studio",
    "codegen": "graphql-codegen --config codegen.yml",
    "log:schema": "npx ts-node src/graphql/schema.ts",    
    "admin": "ts-node scripts/manage-admin.ts"
  },
